#Own udev rule for HP Laserjet 1000
SUBSYSTEMS=="usb", ATTRS{idVendor}=="03f0", \
	ATTRS{product}=="hp LaserJet 1000", \
	SYMLINK+="hplj1000-%n", RUN+="/usr/sbin/hplj1000" 
